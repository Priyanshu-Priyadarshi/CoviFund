<%- include('components/donatesection'); -%>

<section id="precautions" style="background: linear-gradient(135deg, #e0f7fa, #e0f2f1); padding: 40px 20px; text-align: center; border-radius: 12px; box-shadow: 0 6px 24px rgba(0, 0, 0, 0.15); margin-bottom: 0px; display: flex; align-items: center; gap: 20px;">
    <div style="flex: 1; text-align: center;">
        <img src="/assets/img/covidshield.jpg" alt="Covid Shield" style="max-width: 100%; border-radius: 12px; box-shadow: 0 4px 16px rgba(0, 0, 0, 0.1);">
    </div>
    <div style="flex: 2;">
        <h2 style="color: #007580; font-size: 2rem; font-weight: 900; margin-bottom: 20px;">Precautions to Take</h2>
        <ul style="list-style: none; padding: 0; max-width: 800px; margin: 0 auto; text-align: left;">
            <li style="margin-bottom: 12px; font-size: 1.1rem; color: #333; display: flex; align-items: center;">
                <img src="data:image/jpeg;base64," alt="Mask Icon" style="width: 24px; height: 24px; margin-right: 12px;">
                Always wear a mask in public places to protect yourself and others.
            </li>
            <li style="margin-bottom: 12px; font-size: 1.1rem; color: #333; display: flex; align-items: center;">
                <img src="data:image/jpeg;base64," alt="Distance Icon" style="width: 24px; height: 24px; margin-right: 12px;">
               Keep a safe distance of at least 6 feet from others.
            </li>
            <li style="margin-bottom: 12px; font-size: 1.1rem; color: #333; display: flex; align-items: center;">
                <img src="data:image/jpeg;base64," alt="Wash Hands Icon" style="width: 24px; height: 24px; margin-right: 12px;">
                Use soap and water or an alcohol-based sanitizer.
            </li>
            <li style="margin-bottom: 12px; font-size: 1.1rem; color: #333; display: flex; align-items: center;">
                <img src="data:image/jpeg;base64," alt="Crowd Icon" style="width: 24px; height: 24px; margin-right: 12px;">
                 Stay away from gatherings and crowded areas.
            </li>
            <li style="margin-bottom: 12px; font-size: 1.1rem; color: #333; display: flex; align-items: center;">
                <img src="data:image/jpeg;base64," alt="Home Icon" style="width: 24px; height: 24px; margin-right: 12px;">
               If you feel sick, isolate yourself and seek medical advice.
            </li>
        </ul>
    </div>
</section>

